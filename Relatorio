package dao;

import java.io.Serializable;
import java.util.Date;


public class Relatorio implements Serializable{
    private static final long serialVersionUID = 1L;
    private Date data;

    public Relatorio() {}

    public Relatorio(Date data) {
        this.data = data;
    }

    public Date getData() {
        return data;
    }

    public void setData(Date data) {
        this.data = data;
    } 
}
